<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!--
    	Note: text-size-in-dp
    	Some text sizes are specified using dp instead of sp in order to properly handle large system font setting 
    -->

    <style name="fill">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="block">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="wrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="vertical_bar">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="button_clear_container" parent="block">
        <item name="android:paddingLeft">7dp</item>
        <item name="android:paddingRight">7dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingTop">4dp</item>
    </style>

    <style name="tertiary_button" parent="block">
        <item name="android:background">@drawable/tertiary_button_selector</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">@drawable/tertiary_button_text_selector</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minHeight">40dp</item>
    </style>

    <style name="primary_button" parent="block">
        <item name="android:background">@drawable/cta_button_selector</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:minHeight">40dp</item>
    </style>

    <style name="button" parent="tertiary_button" />

    <style name="button.menu">
        <item name="android:textColor">@drawable/tertiary_button_text_selector</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:background">@drawable/tertiary_button_selector</item>
    </style>

    <style name="button.ldpDefaultButton">
        <item name="android:background">@drawable/tertiary_button_selector</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">@drawable/tertiary_button_text_selector</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minHeight">40dp</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="button.ldpDefaultButton.smaller">
        <item name="android:textSize" tools:ignore="SpUsage">13dp</item>
    </style>

    <style name="flat_button" parent="button.ldpDefaultButton.smaller">
        <item name="android:background">@null</item>
    </style>

    <style name="left_aligned_flat_button" parent="flat_button">
        <item name="android:layout_margin">0dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:minHeight">20dp</item>
    </style>

    <style name="button.ldpDefaultButton.smaller.noborder">
        <item name="android:background">@null</item>
    </style>

    <style name="button.mapLayer">
        <item name="android:background">@drawable/tertiary_button_selector</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textColor">@drawable/tertiary_button_text_selector</item>
        <item name="android:minHeight">40dp</item>
    </style>

    <style name="button.defaultInToolbar">
        <item name="android:background">@drawable/cta_button_selector</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="button.defaultInToolbar.twoInARow">
        <item name="android:layout_width">120dp</item>
    </style>

    <style name="srp_cell_summary_section" parent="block">
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">5dp</item>
    </style>

    <style name="srp_cell_summary_section_row" parent="block">
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="srp_cell_summary_section_row.open_house">
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
    </style>

    <style name="srp_actionbar_item_radiogroup" parent="wrap">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">@dimen/row_shim_width_default</item>
        <item name="android:paddingRight">@dimen/row_shim_width_default</item>
    </style>

    <style name="srp_photo_cell" parent="block">
        <item name="android:layout_height">@dimen/srp_photo_cell_height</item>
        <item name="android:paddingTop">@dimen/srp_photo_cell_border_width</item>
    </style>

    <style name="srp_photo_cell_text_info" parent="wrap">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/srp_cell_medium_text</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="srp_photo_cell_show_case_strip">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">5dp</item>
        <item name="android:background">#FEB41C</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="srp_actionbar_item_radio" parent="wrap">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/srp_map_list_photo_bg</item>
        <item name="android:minWidth">50dp</item>
    </style>

    <style name="photo_swipe_tooltip" parent="wrap">
        <item name="android:background">@drawable/photo_cell_swipe_tooltip_bg</item>
        <item name="android:padding">@dimen/row_shim_width_default</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="header_text" parent="@style/vertical_bar">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/actionbar_title</item>
        <item name="android:textSize">@dimen/textSize_header_mainText</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="header_main_text" parent="header_text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">3dp</item>
    </style>

    <style name="header_secondary_text" parent="header_text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/header_secondary_text</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="header_secondary_text.scout">
        <item name="android:textSize">@dimen/scout_header_secondary_text</item>
    </style>

    <style name="scout_header_text" parent="@style/block">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textSize">10sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="header_small_text" parent="header_text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize" tools:ignore="SpUsage">15dp</item>
    </style>

    <style name="header_small_normal_text" parent="header_small_text">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="real_estate_count" parent="wrap">
        <item name="android:textColor">@color/primary</item>
        <item name="android:textSize">14sp</item>
        <item name="android:minWidth">30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="section_header_text" parent="wrap">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="section_header_underline">
        <item name="android:background">@color/separator</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="search_section_header_text" parent="block">
        <item name="android:background">@color/system_background</item>
        <item name="android:textColor">@color/ldp_section_header_text</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">18sp</item>
    </style>

    <!-- seperator line style definations -->
    <style name="separator_line">
        <item name="android:background">@color/grey_300</item>
        <item name="android:layout_height">@dimen/separator_size</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="separator_line.lighter">
        <item name="android:background">@color/grey_300</item>
    </style>

    <style name="separator_line.dark">
        <item name="android:background">@color/dark_grey</item>
    </style>

    <style name="separator_line.dark.for_actionbar_bottom">
        <item name="android:background">@color/black</item>
    </style>

    <style name="separator_line.vertical">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">@dimen/separator_size</item>
    </style>

    <style name="separator_line.vertical.lighter">
        <item name="android:background">@color/grey_300</item>
    </style>

    <style name="separator_line.list_section_header_bottom_border">
        <item name="android:background">@color/real_estate_section_header_bottom_view_color</item>
        <item name="android:layout_height">@dimen/list_section_header_bottom_border_width</item>
    </style>

    <style name="ldp_subsection_title">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">@dimen/ldp_section_title_width</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textColor">@color/ldp_subsection_title</item>
    </style>

    <style name="ldp_key_info_block">
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">@dimen/row_shim_width_default</item>
        <item name="android:background">#5D5E5E</item>
    </style>

    <style name="ldp_key_info_text" parent="@style/wrap">
        <item name="android:textColor">@color/textColor_value_label</item>
        <item name="android:paddingRight">@dimen/row_shim_width_default</item>
    </style>

    <style name="ldp_key_info_value" parent="@style/wrap">
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="table" parent="block">
        <item name="android:shrinkColumns">*</item>
        <item name="android:stretchColumns">*</item>
    </style>

    <style name="table.withBottomBorder">
        <item name="android:background">@drawable/row_with_bottom_border_bg</item>
        <item name="android:paddingBottom">@dimen/row_shim_width_default</item>
        <item name="android:paddingTop">@dimen/row_shim_width_default</item>
    </style>

    <style name="table_row" parent="block">
        <item name="android:paddingLeft">@dimen/row_shim_width_default</item>
        <item name="android:paddingRight">@dimen/row_shim_width_default</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="table_row.noPadding">
        <item name="android:paddingLeft">@null</item>
        <item name="android:paddingRight">@null</item>
    </style>

    <style name="table_row.shaded">
        <item name="android:background">@color/backgroundColor_ldp_overviewStrips</item>
    </style>

    <style name="table_row.with_bottom_border">
        <item name="android:background">@drawable/row_with_bottom_border_bg</item>
    </style>

    <style name="table_row.shaded.with_bottom_border">
        <item name="android:background">@drawable/row_with_bottom_border_bg</item>
    </style>

    <style name="table_row.header">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">3dp</item>
    </style>

    <style name="table_cell" parent="block" />

    <style name="table_cell.rightAlign">
        <item name="android:layout_gravity">right</item>
        <item name="android:paddingRight">20dp</item>
    </style>

    <style name="table_cell.strong">
        <item name="android:textStyle">bold</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="table_cell.header">
        <item name="android:textSize" tools:ignore="SpUsage">12dp</item>
        <item name="android:textColor">@color/grey</item>
    </style>

    <style name="table_cell.header.rightAlign">
        <item name="android:layout_gravity">right</item>
        <item name="android:paddingRight">20dp</item>
    </style>

    <style name="great_school_rating" parent="wrap">
        <item name="android:background">@drawable/great_school_rating_decobottom_toolbar_bg</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="email_form_label">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:textSize" tools:ignore="SpUsage">18dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="radio_list_row" parent="block">
        <item name="android:paddingLeft">@dimen/row_shim_width_default</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:layout_height">@dimen/row_height_normal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="radio_list_text" parent="block">
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/list_row_text_size_default</item>
    </style>

    <style name="radio_list_icon" parent="wrap">
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="checked_list_text" parent="radio_list_text">
        <item name="android:layout_marginTop">12dp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize" tools:ignore="SpUsage">16dp</item>
    </style>

    <style name="search_list_text" parent="radio_list_text">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="search_list_checkbox" parent="right_side_checkbox">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="widget_arrow" parent="@style/fill">
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="list_section_body" parent="block">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="my_real_estate_list_section_header" parent="block">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/real_estate_section_header_background</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="dashboard_button" parent="block">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/primary</item>
        <item name="android:minLines">2</item>
        <item name="android:background">@drawable/dashboard_button_selector</item>
    </style>

    <style name="dashboard_button_top" parent="dashboard_button">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>

    <!-- See text-size-in-dp note above -->
    <style name="splash_dashboard_button">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize" tools:ignore="SpUsage">14dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:background">@drawable/dashboard_button_selector</item>
    </style>

    <style name="no_recently_viewed_listings_text" parent="block">
        <item name="android:layout_width">200dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">@color/black</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <style name="tip_dot">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="arrow_left_button" parent="wrap">
        <item name="android:layout_gravity">left</item>
    </style>

    <style name="arrow_right_button" parent="wrap">
        <item name="android:layout_gravity">right</item>
    </style>

    <style name="toolbar">
        <item name="android:layout_height">@dimen/toolbar_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="bottom_toolbar" parent="toolbar">
        <item name="android:background">@drawable/bottom_toolbar_bg</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="ldp_bottom_toolbar">
        <item name="android:background">@color/grey_900</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/ldp_toolbar_height</item>
    </style>

    <style name="ldp_tab" parent="block">
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">@dimen/row_shim_width_default_half</item>
        <item name="android:paddingBottom">@dimen/row_shim_width_default_half</item>
    </style>

    <style name="toolbar_item">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="toolbar_item_text" parent="wrap">
        <item name="android:paddingTop">1dp</item>
        <item name="android:layout_gravity">center_vertical|center_horizontal</item>
        <item name="android:textSize">11sp</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">-2dp</item>
    </style>

    <style name="Button" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/cta_button_selector</item>
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
    </style>

    <style name="ToggleButton" parent="@android:style/Widget.Button.Toggle">
        <item name="android:background">@drawable/tertiary_button_selector</item>
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:disabledAlpha">?android:attr/disabledAlpha</item>
    </style>

    <style name="ImageButton" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/cta_button_selector</item>
    </style>

    <style name="lead_form_tab">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text</item>
    </style>

    <style name="lead_form_tab_weighted" parent="lead_form_tab">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="lead_form" parent="block">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/lead_bg_light</item>
    </style>

    <style name="lead_form_label" parent="wrap">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">@dimen/row_height_normal</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="dialog_lead_form_label" parent="wrap">
        <item name="android:textColor">@color/darker_grey</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

